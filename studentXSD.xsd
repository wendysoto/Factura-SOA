<xs:schema version="1.0"
           xmlns:xs="http://www.w3.org/2001/XMLSchema"
           elementFormDefault="qualified">
    <xs:element name="estudiante">
        <xs:complexType>
            <xs:sequence>
                <xs:element name="identificacion" type="identificacionType"/>
                <xs:element name="genero" type="xs:string"/>
                <xs:element name="telefono" type="xs:string"/>
                <xs:element name="direccion" type="xs:string"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>
    <xs:complexType name="identificacionType">
        <xs:sequence>
            <xs:element name="pri_nom" type="xs:string"/>
            <xs:element name="seg_nom" type="xs:string"/>
            <xs:element name="pri_ape" type="xs:string"/>
            <xs:element name="seg_ape" type="xs:string"/>
        </xs:sequence>
    </xs:complexType>
</xs:schema>
